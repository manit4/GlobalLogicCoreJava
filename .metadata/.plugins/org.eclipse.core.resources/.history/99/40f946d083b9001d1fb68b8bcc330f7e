package com.global;

public class Human {
	
	int iq = 50;
	int mascularStrength = 20;
	int height = 1;
	
	public Human(int val1, int val2, int val3) {
		
		iq = val1;
		mascularStrength = val2;
		height = val3;
	}
	
	public Human() {
		
	}
	
	
	public void study() {
		
		iq++;
	}
	
	void playingBasketball() {
		
		height++;
	}
	
	void gyming() {
		
		mascularStrength++;
	}

	public int getIq() {
		return iq;
	}

	public int getMascularStrength() {
		return mascularStrength;
	}

	public int getHeight() {
		return height;
	}
	
	public boolean equals(Human obj) {
		
		System.out.println("inside equals");
		
		//Human csatedObj = (Human)obj;
		
		if(iq == obj.iq) {
			
			return true;
		}
		else {
			return false;
		}
	}
	
	public String toString() {
		
		return iq+", "+mascularStrength+", "+height;
	}
	
	

}

package com.global;

class Human {
	
	int iq = 20;
	int mascularStrength = 50;
	int height = 1;
	
	public void study() {
		
		iq++;
	}
	
	public void playingBasketball() {
		
		height++;
	}
	
	public void gyming() {
		
		mascularStrength++;
	}
}

public class Serialization {

	public static void main(String[] args) {
		
		Human h1 = new Human();
		h1.study();    h1.study();     h1.study();      h1.gyming();    h1.gyming();   h1.playingBasketball();
	}
}
